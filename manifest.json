{
	"name": "Adventer_BookMark",
	"version": "0.0.1",
	"manifest_version": 3,
	"description": "Add Bookmark in Adventer site",
	"permissions":[
		"storage"
	],
	"content_scripts": [{
		"matches": ["https://adventar.org/*"],
		"css": [
			"src/style.css"
		],
		"js": [
			"lib/jquery-3.6.0.min.js",
			"src/content.js"
		]
	}]
}
